{
    "name": "sebble/flarum-ext-oauth-gitlab",
    "description": "Allow users to log in with GitLab.",
    "type": "flarum-extension",
    "keywords": ["authentication"],
    "license": "GPL-3.0+",
    "version": "v0.1.0-beta.8.2.2+sebble",
    "authors": [
        {
            "name": "John Hearfield",
            "email": "dev@findingjohn.xyz"
        },
        {
            "name": "Sebastian Mellor",
            "email": "sebble@sebble.com"
        }
    ],
    "support": {
        "issues": "https://github.com/sebble/flarum-ext-oauth-gitlab/issues",
        "source": "https://github.com/sebble/flarum-ext-oauth-gitlab"
    },
    "require": {
        "flarum/core": "^0.1.0-beta.6",
        "omines/oauth2-gitlab": "^1.1.0"
    },
    "autoload": {
        "psr-4": {
            "JohnHearfield\\Auth\\Google\\": "src/"
        }
    },
    "extra": {
        "flarum-extension": {
            "title": "GitLab Login",
            "icon": {
                "name": "gitlab",
                "backgroundColor": "#554488",
                "color": "#fff"
            }
        }
    }
}
